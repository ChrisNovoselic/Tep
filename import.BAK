PROC norm
PRIV recno,RealNumHour
IF 'PAR_TPM'$NameFile
   nmonth=IIF(nmonth=1,12,nmonth-1)
ENDIF
recno=recno()
GO TOP
IF SUBSTR(&x,1,1)#'s'
