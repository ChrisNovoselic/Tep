	DIMEN NumHour(12)
	IF 'PAR_TPD'$NameFile
		STORE 24 TO NumHour
*!*			FOR i = 1 TO 12
*!*				NumHour (i) = 24
*!*			endfor
   	ELSE
	   FOR i=1 TO 12  &&           I  II III  IV   V  VI VII VIII IX   X  XI XII
	       NumHour(i)=VAL(SUBS(' 744 672 744 720 744 720 744 744 720 744 720 744',;
	       4*(i-1)+1,4))
	   ENDFOR
	ENDIF
ENDIF
COPY FILE (NameFile) TO WRK.WRK
n = FOPEN('WRK.WRK',0)
sdata=CTOD(SUBSTR(FGETS(n),1,8))
nmonth=MONTH(sdata)
FOR i=2 TO 5                    && Со 2 по 5 строки во вх.файле -
    = FGETS(n)                  && служебная информация
ENDFOR    
DO WHILE .T.
   inblok=FGETS(n)
   IF FEOF(n) &&SUBSTR(inblok,63,7)==''
      EXIT
   ENDIF
   IF SUBSTR(inblok,63,7)==''
      LOOP
   ENDIF
   x=SUBSTR(inblok,6,7)
   y=SUBSTR(inblok,1,4)
   accuracy=IIF(ALLTRIM(y)=='30',3,2)
   IF ALLTRIM(y)==''
      LOOP
   ENDIF
   LOCATE FOR ALLTRIM(SUBSTR(order,2))==ALLTRIM(y)
   DO CASE
      CASE BL1 AND ALLTRIM(y)=='59'  && ОСОБЫЙ СЛУЧАЙ: норматив - средневзвешиваемый,
                                      * но надо суммировать, причем только для '/1'
           z=ROUND(VAL(SUBSTR(inblok,60,10))+VAL(&x)*IIF(Iter=1,0,1),accuracy)
      CASE sum_norm      && норматив - суммируемый
           z=ROUND(VAL(SUBSTR(inblok,60,10))+VAL(&x)*IIF(Iter=1,0,1),accuracy)
      CASE NOT sum_norm  && норматив - средневзвешиваемый
           z=ROUND((VAL(SUBSTR(inblok,60,10))+VAL(&x)*(Iter-1))/Iter,accuracy)
           IF ('PAR_TPN'$NameFile OR 'PAR_TPM'$NameFile OR 'PAR_TPD'$NameFile);
              AND INLIST(x,'blok5')
              DO norm
           ENDIF
   ENDCASE
   IF SUBSTR(&x,1,1)#'s'
      REPLACE &x WITH STR(z,10,accuracy)
   ENDIF
ENDDO
